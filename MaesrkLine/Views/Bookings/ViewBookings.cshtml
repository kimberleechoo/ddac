@model IEnumerable<MaesrkLine.Models.Booking>

@{
    ViewBag.Title = "ViewBookings";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CustomerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CustomerEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContainerType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContainerSize)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VesselName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CustomerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CustomerEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContainerType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContainerSize)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VesselName)
        </td>
        <td>
          
        
        </td>
    </tr>
}

</table>
